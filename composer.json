{
    "name": "oseintow/laravel-shopify",
    "description": "Laravel shopify package",
    "license": "MIT",
    "authors": [
        {
            "name": "michael osei ntow",
            "email": "oseintow@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/support": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Oseintow\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oseintow\\Shopify\\ShopifyServiceProvider"
            ],
            "aliases": {
                "Shopify": "Oseintow\\Shopify\\Facades\\Shopify"
            }
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
